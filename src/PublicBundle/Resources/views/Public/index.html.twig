{% extends "::base.html.twig" %}


{% block stylesheets %}
    <link rel="stylesheet" href="{{ asset('bundles/public/css/screen.css') }}" type="text/css" media="screen">
    <!--[if lte IE 8]><script src="assets/js/ie/html5shiv.js"></script><![endif]-->
    <link rel="stylesheet" href="{{ asset('bundles/public/assets/css/main.css') }}" type="text/css" />	
    <!--[if lte IE 9]><link rel="stylesheet" href="assets/css/ie9.css" /><![endif]-->
    <!--[if lte IE 8]><link rel="stylesheet" href="assets/css/ie8.css" /><![endif]--> 
{% endblock %}

{% block body %}
    <!-- Page Wrapper -->
    <div id="page-wrapper">
        {% block header %}
            <!-- Header -->
            <header id="header" class="alt">
                <h1><a href="http://localhost/Beezyweb/web/app_dev.php/">Acceuil</a></h1>
                <nav>
                    <a href="#menu">Menu</a>
                </nav>
            </header>
        {% endblock %}
        <!-- Menu -->
        <nav id="menu">
            <div class="inner">
                <h2>Menu</h2>
                <ul class="links">
                    <li class="nada"><a href="http://localhost/Beezyweb/web/app_dev.php/">Accueil</a></li> 
                    <li class="nada"><a href="">A Propos</a></li>                                            			
                    <li id="calendrier"><a href="#" onclick="afficheCalendrier()">Calendrier</a></li>
                    <li><a href="{{ path('fos_user_security_login')}}">Connexion</a></li>							
                    <li><a href="{{path('fos_user_registration_register')}}">S'enregistrer</a></li>
                </ul>
                <a href="#" class="close">Close</a>
            </div>
        </nav>
        <section id="contenu">
            {% block contenu %} 
                <!-- Banner -->
                <section id="banner">
                    <div class="inner">
                        <div class="logo"><span class="icon fa-diamond"></span></div>
                        <h2>Beezyweb</h2>
                        <p>Planning de réservation en ligne.</p>
                    </div>
                </section>

                <!-- Wrapper -->            					
                <section id="wrapper">

                    <!-- One -->
                    <section id="one" class="wrapper spotlight style1">
                        <div class="inner">
                            <a href="#" class="image"><img src="{{ asset('bundles/public/images/pic01.jpg')}}" alt="" /></a>
                            <div class="content">
                                {% block wrapperContent %}
                                    <h2 class="major">Auto-Ecole</h2>
                                    <p><strong>Beezweb</strong> c’est avant tout VOTRE plateforme de gestion des prises de rendez vous entièrement en ligne.
                                        Notre volonté ? Un suivi instantané de vos réservations d'heures de conduite en temps et en heure depuis votre espace personnel.														
                                        Terminé les appels, texto vers votre agence auto-école pour réserver une heure de conduite.
                                        Faite le désormais à l' aide de votre ordinateur,tablette ou smartphone depuis votre canapé.
                                    </p>

                                {% endblock %}           										
                            </div>
                        </div>
                    </section>            							
                </section>             					          				
            {% endblock %}
        </section>

        <!-- Footer -->
        {% block footer %}
            <section id="footer">						
                <div class="inner">
                    <h2 class="major">Contact</h2>		
                    <form method="post" action="">
                        <div class="field">
                            <label for="name">Nom</label>
                            <input required="required" type="text" name="nom" id="name" />
                        </div>
                        <div class="field">
                            <label for="email">Email</label>
                            <input required="required" type="email" name="email" id="email" />
                        </div>
                        <div class="field">
                            <label for="sujet">Sujet</label>
                            <input required="required" type="text" name="sujet" id="sujet" />
                        </div>								
                        <div class="field">
                            <label for="message">Message</label>
                            <textarea required="required" name="message" id="message" rows="4"></textarea>
                        </div>
                        <ul class="actions">
                            <li><input type="submit" value="Envoyer" /></li>
                        </ul>
                    </form>
                    <ul class="contact">
                        <li class="fa-home">
                            NOUMIA KEWOU<br />
                            14200 <br />
                            Hérouville Sain-Clair<br />
                            1021
                            Bd des Belles-portes
                        </li>
                        <li class="fa-phone">(+33) 6 15 66 47 58 </li>
                        <li class="fa-envelope"><a href="#">information@untitled.tld</a></li>
                        <li class="fa-twitter"><a href="#">twitter.com/untitled-tld</a></li>
                        <li class="fa-facebook"><a href="https://www.facebook.com/Beezy-Start-up-1459462844287068/">facebook.com/untitled-tld</a></li>								
                    </ul>
                    <ul class="copyright">
                        <li>&copy; NOUMIA KEWOU. All rights reserved.</li><li>2015-{{ 'now'|date('Y') }}</li>
                    </ul>
                </div>						
            </section>
        {% endblock %}

    </div>
    {% block javascripts %}
        <!-- Scripts -->
        <script type="text/javascript" src="{{ asset('bundles/public/assets/js/jquery.min.js')}}"></script>
        <script type="text/javascript" src="{{ asset('bundles/public/bootstrap/js/bootstrap.min.js')}}"></script>
        <script type="text/javascript" src="https://cdn.jsdelivr.net/jquery.validation/1.15.1/jquery.validate.min.js"></script>
        <script src="http://cdn.jsdelivr.net/jquery.validation/1.15.0/additional-methods.min.js"></script>
        <script type="text/javascript" src="{{ asset('bundles/public/js/validationForm/valid.js')}}"></script>
        <script type="text/javascript" src="{{ asset('bundles/public/js/validationForm/messages_fr.js')}}"></script>        	               
        <script type="text/javascript" src="{{ asset('bundles/reservation/js/calendrier.js')}}"></script>            
        <script type="text/javascript" src="{{ asset('bundles/public/assets/js/skel.min.js')}}"></script>
        <script type="text/javascript" src="{{ asset('bundles/public/assets/js/skel.min.js')}}"></script>         	
        <script type="text/javascript" src="{{ asset('bundles/public/assets/js/jquery.scrollex.min.js')}}"></script>        	               
        <script type="text/javascript" src="{{ asset('bundles/public/assets/js/util.js')}}"></script>
        <script type="text/javascript" src="{{ asset('bundles/public/assets/js/main.js')}}"></script>
        <script type="text/javascript" src="{{ asset('bundles/public/js/toTop/js/easing.js')}}"></script> 
        <script type="text/javascript" src="{{ asset('bundles/public/js/toTop/js/jquery.ui.totop.js')}}"></script> 
        <script type="text/javascript">
                        $(document).ready(function () {
                            var defaults = {
                                containerID: 'toTop', // fading element id
                                containerHoverID: 'toTopHover', // fading element hover id
                                scrollSpeed: 1200,
                                easingType: 'linear'};
                            $().UItoTop({easingType: 'easeOutQuart'});
                        });
        </script>                        		
    {% endblock %}
{% endblock %}





